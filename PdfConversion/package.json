{
  "name": "pdf-conversion",
  "version": "1.0.0",
  "description": "PDF to Taxxor TDM XHTML Conversion Tool",
  "scripts": {
    "start": "dotnet watch run --verbose --no-hot-reload",
    "build": "dotnet build",
    "test": "dotnet test",
    "clean": "dotnet clean",
    "restore": "dotnet restore",
    "publish": "dotnet publish -c Release -o ./publish",
    "docker:build": "cd .. && docker-compose build pdfconversion",
    "docker:up": "cd .. && docker-compose up -d pdfconversion xslt3service",
    "docker:down": "cd .. && docker-compose down",
    "docker:logs": "cd .. && docker logs -f tdm-pdfconversion-1",
    "docker:restart": "cd .. && docker-compose restart pdfconversion",
    "docker:rebuild": "cd .. && docker-compose down && docker-compose build pdfconversion && docker-compose up -d pdfconversion xslt3service",
    "start-docker": "npm run docker:logs"
  },
  "keywords": [
    "pdf",
    "xslt",
    "blazor",
    "taxxor"
  ],
  "author": "Optiver",
  "license": "UNLICENSED",
  "private": true
}